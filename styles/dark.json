{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "title": "Dark",
    "settings": {
        "appearanceTools": true,
        "color": {
            "palette": [
                {
                    "slug": "primary",
                    "color": "#67A61E",
                    "name": "Primary"
                },
                {
                    "slug": "secondary",
                    "color": "#FFFFFF",
                    "name": "Secondary"
                },
                {
                    "slug": "Review",
                    "color": "#F16522",
                    "name": "review"
                },
                {
                    "slug": "background",
                    "color": "#252525",
                    "name": "Background"
                },
                {
                    "slug": "grey",
                    "color": "#828282",
                    "name": "Grey"
                },
                {
                    "slug": "Border",
                    "color": "#FFFFFF",
                    "name": "Border"
                },
                {
                    "name": "BG Color",
                    "color": "#000000",
                    "slug": "bg-color"
                },
                {
                    "name": "Button Hover",
                    "color": "#528518",
                    "slug": "btn-hover"
                }
            ],
            "duotone": [
                {
                    "colors": [
                        "#FFFFFF",
                        "#FFFFFF"
                    ],
                    "slug": "primary",
                    "name": "Primary"
                },
                {
                    "colors": [
                        "#7DCA24",
                        "#67A61E"
                    ],
                    "slug": "secondary",
                    "name": "Secondary"
                },
                {
                    "colors": [
                        "#000000",
                        "#000000"
                    ],
                    "slug": "tertiary",
                    "name": "Tertiary"
                }
            ]
        }
    },
    "styles": {
        "color": {
            "background": "var(--wp--preset--color--black)",
            "text": "var(--wp--preset--color--white)"
        },
        "elements": {
            "button": {
                "color": {
                    "background": "var(--wp--preset--color--secondary)",
                    "text": "var(--wp--preset--color--bg-color)"
                },
                ":hover": {
                    "color": {
                        "background": "var(--wp--preset--color--btn-hover)",
                        "text": "var(--wp--preset--color--secondary)"
                    }
                }
            },
            "heading": {
                "typography": {
                    "fontWeight": "700",
                    "lineHeight": "1.3"
                },
                "color": {
                    "text": "var(--wp--preset--color--secondary)"
                }
            }
        }
    }
}